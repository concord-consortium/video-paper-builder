<% unless @video.nil? %>
  <% if @video.processed? %>
    <% if @video.private? && !owner? %>
      this video is private.
    <% else %>
      <%= kaltura_player_embed @video.entry_id, :div_id => "kplayer_duration", :player_conf_id => KalturaFu.config[:duration_modal_player_conf_id] %>
    <% end %>
  <% else %>
    The video is still processing.
  <% end %>
<% end %>